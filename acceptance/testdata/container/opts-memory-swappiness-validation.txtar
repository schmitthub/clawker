# Test memory swappiness validation (--memory-swappiness range)
# Verifies: Invalid memory swappiness values are rejected (valid range 0-100)

# Substitute project name in config
replace clawker.yaml PROJECT=$PROJECT

# Create settings directory
mkdir $HOME/.local/clawker

# Test above maximum
! exec clawker container create --agent swpv-$RANDOM_STRING --memory-swappiness 200 alpine:latest sleep 3600
stderr 'swappiness'

-- clawker.yaml --
version: "1"
project: "$PROJECT"

build:
  image: "alpine:latest"

security:
  firewall:
    enable: false
